{
    "name": "bekk-christmas",
    "private": true,
    "description": "A simplified bare-bones starter for Gatsby",
    "version": "0.1.0",
    "license": "MIT",
    "scripts": {
        "build": "cross-env GATSBY_EXPERIMENTAL_PAGE_BUILD_ON_DATA_CHANGES=true gatsby build --log-pages",
        "build:preview": "cross-env GATSBY_EXPERIMENTAL_PAGE_BUILD_ON_DATA_CHANGES=true CALENDAR_ENV=preview gatsby build",
        "build:calendar": "cross-env gatsby clean && gatsby build",
        "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
        "start": "gatsby develop",
        "start:preview": "cross-env CALENDAR_ENV=preview gatsby develop",
        "start:calendar": "cross-env gatsby clean && gatsby develop",
        "serve": "gatsby serve",
        "clean": "gatsby clean"
    },
    "dependencies": {
        "@bekk/storybook": "^2.5.1",
        "@gatsby-contrib/gatsby-plugin-elasticlunr-search": "^2.4.2",
        "babel-plugin-styled-components": "^1.11.1",
        "gatsby": "^2.24.53",
        "gatsby-plugin-feed": "^2.5.11",
        "gatsby-plugin-google-analytics": "^2.3.13",
        "gatsby-plugin-netlify": "^2.3.13",
        "gatsby-plugin-netlify-cms": "^4.3.12",
        "gatsby-plugin-react-helmet": "^3.3.10",
        "gatsby-plugin-styled-components": "^3.3.10",
        "gatsby-remark-emoji": "0.0.3",
        "gatsby-remark-numbered-footnotes": "^1.0.1",
        "gatsby-remark-prismjs": "^3.5.11",
        "gatsby-remark-responsive-iframe": "^2.4.12",
        "gatsby-source-filesystem": "^2.3.27",
        "gatsby-transformer-remark": "^2.8.32",
        "netlify-cms-app": "^2.13.3",
        "prismjs": "^1.21.0",
        "react": "^16.13.1",
        "react-dom": "^16.13.1",
        "react-helmet": "^5.2.1",
        "remark": "^11.0.2",
        "remark-html": "^10.0.0",
        "remark-preset-lint-recommended": "^3.0.4",
        "striptags": "^3.1.1",
        "styled-components": "^5.2.0",
        "styled-normalize": "^8.0.7"
    },
    "devDependencies": {
        "cross-env": "^7.0.2",
        "eslint-config-react-app": "^5.2.1",
        "prettier": "^2.1.1"
    }
}
